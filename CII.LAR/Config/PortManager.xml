<?xml version="1.0" encoding="utf-8"?>
<PortManager>
	<pipes>
		<pipe>
			<id>HVPipe</id>
			<name>HVPipe</name>
			<valid>True</valid>
			<autoConnect>True</autoConnect>
			<autoReConnect>True</autoReConnect>
			<impBus>CII.Library.CIINet.Buses.SPCommBus</impBus>
			<impProtocol>CII.Ins.Model.Protocols.InsCommunicationProtol</impProtocol>
			<targets>
				<target>LAR</target>
			</targets>
			<propertys>
				<property>
					<id>CII.Ins.Model.Protocols.InsCommunicationProtol</id>
					<name>协议配置</name>
					<propertys>
						<property>
							<id>CII.Library.CIINet.Ports.CIIPorts.CIIRouterPort</id>
							<name>地址配置</name>
							<propertys>
								<property>
									<id>address</id>
									<name>new Name</name>
									<value>0xF2</value>
								</property>

							</propertys>

						</property>

						<property>
							<id>Receiver</id>
							<name>Receiver</name>
							<value>CII.Ins.Business.Receive.InsReceiver</value>
						</property>

						<property>
							<id>CII.Library.CIINet.Ports.SyncPorts.SyncPort</id>
							<name>同步Key配置</name>
							<propertys>
								<property>
									<id>resendKey</id>
									<name>resendKey</name>
									<value>CII.Library.CIINet.Ports.SyncPorts.ResendKeys.CIIResendKey</value>
								</property>

								<property>
									<id>unique</id>
									<name>unique</name>
									<value>true</value>
								</property>

							</propertys>

						</property>

					</propertys>

				</property>

				<property>
					<id>CII.Library.CIINet.Buses.SPCommBus</id>
					<name>链路配置</name>
					<propertys>
						<Property>
							<id>port</id>
							<name>port</name>
							<value>COM3</value>
						</Property>

						<Property>
							<id>baud</id>
							<name>baud</name>
							<value>115200</value>
						</Property>

						<Property>
							<id>dataBit</id>
							<name>dataBit</name>
							<value>8</value>
						</Property>

						<Property>
							<id>stopBit</id>
							<name>stopBit</name>
							<value>1</value>
						</Property>

					</propertys>

				</property>

			</propertys>

		</pipe>

	</pipes>

</PortManager>
